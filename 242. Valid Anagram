class Solution {
    public boolean isAnagram(String s, String t) {
        int flag=0;
        s=s.toLowerCase();
        char[] s1= s.toCharArray();
         Arrays.sort(s1);
       
         t=t.toLowerCase();
        char[] s2= t.toCharArray();
        Arrays.sort(s2);
        
        return Arrays.equals(s1,s2);
      
    }
}
